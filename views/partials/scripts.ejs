<script>
  let data = [
    {
      item: 'Non-Teaching Staff',
      subitems: ['Cashier Office','Clinic','Guidance and Counselling Office','Library','Records Office','Registrar Office', 'Student Services Office', 'Supply Office','Supreme Student Council'],
    },
    {
      item: 'Teaching Staff',
      subitems: ['Architecture Department', 'Civil Engineering Department', 'Computer Engineering Department', 'Electrical Engineering Department', 'Mechanical Engineering Department', 'Teacher Education Department', 'English Language Department', 'General Education Department', 'Physical Education and NSTP Department', 'Mathematics and Natural Sciences Department', 'Information Technology Department']
    },
    {
      item: 'Department Chair',
      subitems: ['Architecture Department', 'Civil Engineering Department', 'Computer Engineering Department', 'Electrical Engineering Department', 'Mechanical Engineering Department', 'Teacher Education Department', 'English Language Department', 'General Education Department', 'Physical Education and NSTP Department', 'Mathematics and Natural Sciences Department', 'Information Technology Department']
    },
    {
      item: 'College Dean',
      subitems: ['College of Arts and Education', 'College of Computing', 'College of Engineering and Architecture']
    },
  ];
  
  
  window.onload = function() {
    var itemSel = document.getElementById("type");
    var subitemSel = document.getElementById("designation");
  
    for (var x in data) {
      itemSel.options[itemSel.options.length] = new Option(data[x].item, x);
    }
    itemSel.onchange = function() {
      //empty 
      subitemSel.length = 1;
      //display correct values
      for (var y of data[this.value].subitems) {
        subitemSel.options[subitemSel.options.length] = new Option(y, y);
      }
    }
  }
</script>


